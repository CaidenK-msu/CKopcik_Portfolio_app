<div id="<%= dom_id student %>">
  <p>
    <strong>First Name:</strong>
    <%= student.first_name %>
  </p>

  <p>
    <strong>Last Name:</strong>
    <%= student.last_name %>
  </p>

  <p>
    <strong>Major:</strong>
    <%= student.major %>
  </p>

  <p>
    <strong>Minor:</strong>
    <%= student.minor %>
  </p>

  <p>
    <strong>Graduation date:</strong>
    <%= student.graduation_date %>
  </p>
</div>
